<h1>Listing agreements</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Ipp agreement guid</th>
    <th>Contract manager</th>
    <th>Set up complete</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @agreements.each do |agreement| %>
  <tr>
    <td><%= agreement.name %></td>
    <td><%= agreement.ipp_agreement_guid %></td>
    <td><%= agreement.contract_manager %></td>
    <td><%= agreement.set_up_complete %></td>
    <td><%= link_to 'Show', agreement %></td>
    <td><%= link_to 'Edit', edit_agreement_path(agreement) %></td>
    <td><%= link_to 'Destroy', agreement, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Agreement', new_agreement_path %>
